{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "explorejsonplaceholder": {
      "projectType": "application",
      "root": "apps/explorejsonplaceholder",
      "sourceRoot": "apps/explorejsonplaceholder/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/explorejsonplaceholder",
            "index": "apps/explorejsonplaceholder/src/index.html",
            "main": "apps/explorejsonplaceholder/src/main.ts",
            "polyfills": "apps/explorejsonplaceholder/src/polyfills.ts",
            "tsConfig": "apps/explorejsonplaceholder/tsconfig.app.json",
            "assets": [
              "apps/explorejsonplaceholder/src/favicon.ico",
              "apps/explorejsonplaceholder/src/assets"
            ],
            "styles": ["apps/explorejsonplaceholder/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/explorejsonplaceholder/src/environments/environment.ts",
                  "with": "apps/explorejsonplaceholder/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "explorejsonplaceholder:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "explorejsonplaceholder:build:production"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/explorejsonplaceholder/src/test.ts",
            "polyfills": "apps/explorejsonplaceholder/src/polyfills.ts",
            "tsConfig": "apps/explorejsonplaceholder/tsconfig.spec.json",
            "karmaConfig": "apps/explorejsonplaceholder/karma.conf.js",
            "assets": [
              "apps/explorejsonplaceholder/src/favicon.ico",
              "apps/explorejsonplaceholder/src/assets"
            ],
            "styles": ["apps/explorejsonplaceholder/src/styles.css"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/explorejsonplaceholder/tsconfig.app.json",
              "apps/explorejsonplaceholder/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "explorejsonplaceholder"
}
